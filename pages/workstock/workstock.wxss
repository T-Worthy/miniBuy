}
.navigator-box {
  position: relative;
  background-color: #ffffff;
  line-height: 1.41176471;
  font-size: 34rpx;
  width: 95%;
  height: 80rpx;
  margin-left: 20rpx;
  border-radius: 10rpx;
  border-bottom: 1rpx solid #f3f3f3;
  margin-top: 10rpx
}
.navigator-box-show {
  opacity: 1;
  transform: translateY(0);
}
.navigator {
  /* padding: 30rpx; */
  position: relative;
  display: flex;
  align-items: center;
  
  border-bottom: 1rpx solid #f3f3f3;
  background-color: #ffffff;
}
.navigator:before {
  content: " ";
  position: absolute;
  left: 30rpx;
  top: 0;
  right: 30rpx;
  height: 1px;
  color: #D8D8D8;
}
.navigator:first-child:before {
  display: none;
}
.navigator-text {
  margin-left: 20rpx;
  flex: 1;
}
.navigator-arrow {
  padding-right: 26rpx;
  position: relative;
}
.navigator-arrow:after {
  content: " ";
  display: inline-block;
  height: 18rpx;
  width: 18rpx;
  border-width: 2rpx 2rpx 0 0;
  border-color: #888888;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  /* margin-top: -8rpx; */
  right: 28rpx;
}
.workstock-end{
  margin-top: 30rpx;
}